{
  "name": "conexa-server",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run mkdir:uploads && tsc",
    "start": "npm run mkdir:uploads && node dist/src/index.js",
    "dev": "npm run mkdir:uploads && ts-node-dev src/index.ts",
    "prisma:generate": "prisma generate --schema=../prisma/schema.prisma",
    "mkdir:uploads": "mkdir -p ./uploads",
    "prisma:push": "prisma db push --schema=../prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=../prisma/schema.prisma",
    "prisma:seed": "ts-node --project ./tsconfig.json ../prisma/seed.ts",
    "prisma:migrate": "prisma migrate dev --schema=../prisma/schema.prisma"
  },
  "prisma": {
    "seed": "ts-node --project ./tsconfig.json ../prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.0.0",
    "exceljs": "^4.4.0",
    "express": "^4.18.0",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "pdfkit": "^0.15.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/morgan": "^1.9.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.0.0",
    "@types/pdfkit": "^0.13.4",
    "@types/xml2js": "^0.4.11",
    "prisma": "5.22.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}
